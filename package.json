{
  "name": "@commercetools/csv-parser-price",
  "version": "1.1.4",
  "description": "Module that parses price csv to json",
  "keywords": [
    "commercetools",
    "csv",
    "parser",
    "price",
    "json"
  ],
  "homepage": "https://github.com/commercetools/csv-parser-price#readme",
  "bugs": "https://github.com/commercetools/csv-parser-price/issues",
  "license": "MIT",
  "contributors": [
    {
      "name": "Abimbola Idowu",
      "email": "abimbola.idowu@commercetools.de"
    },
    {
      "name": "Selwyn Versteeg",
      "email": "selwyn.versteeg@commercetools.de"
    }
  ],
  "main": "lib/index.js",
  "bin": {
    "csvparserprice": "bin/csvparserprice.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/csv-parser-price.git"
  },
  "scripts": {
    "start": "npm run test:watch",
    "test": "npm run build && jest",
    "test:watch": "jest --watch",
    "build": "npm run clean && babel src bin -d lib",
    "clean": "rimraf lib/*",
    "commit": "git-cz || true",
    "codecov": "cat ./coverage/lcov.info | codecov",
    "coverage": "npm run build && jest --coverage",
    "lint": "./scripts/lint src test",
    "prepublish": "npm run build"
  },
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": ">= 4.5.0"
  },
  "dependencies": {
    "@commercetools/api-request-builder": "^1.2.0",
    "@commercetools/sdk-client": "^1.2.0",
    "@commercetools/sdk-middleware-auth": "^3.0.0",
    "@commercetools/sdk-middleware-http": "^2.0.0",
    "JSONStream": "^1.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "bluebird": "^3.4.6",
    "csv-parser": "^1.11.0",
    "flat": "^2.0.1",
    "highland": "^2.10.1",
    "lodash.mapvalues": "^4.6.0",
    "lodash.memoize": "^4.1.2",
    "npmlog": "^4.0.1",
    "pretty-error": "^2.0.2",
    "yargs": "^6.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.14.0",
    "chokidar-cli": "^1.2.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.6",
    "cross-env": "^3.1.3",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-commercetools": "^6.0.0",
    "eslint-formatter-pretty": "^1.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.3.0",
    "ghooks": "^2.0.0",
    "jest": "^18.1.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "streamtest": "^1.2.2",
    "tap-spec": "^4.1.1",
    "tmp": "^0.0.31",
    "validate-commit-msg": "^2.8.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "./scripts/lint-staged-files"
    }
  },
  "eslintConfig": {
    "extends": "commercetools",
    "env": {
      "jest": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src/"
          ]
        }
      }
    }
  }
}
